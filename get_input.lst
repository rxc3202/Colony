  75 0040               
  76 0040 8FBF0004	        lw      $ra, 4($sp)
  77 0044 8FB00000	        lw      $s0, 0($sp)                         #restore s register 
  78 0048 23BD0008	        addi    $sp, $sp, 8
  79 004c 03E00008	        jr      $ra
  80 0050               
